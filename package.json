{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "./app.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "targets": {
    "main": false
  },
  "scripts": {
    "watch": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundled  --no-hmr",
    "build": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled",
    "dev": "npx concurrently \"npm run watch\" \"nodemon ./server.js\"",
    "start": "node ./server.js",
    "start:prod": "NODE_ENV=production nodemon ./server.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "author": "Oleh Slyvkanych",
  "license": "ISC",
